{
  "$schema": "https://json.schemastore.org/chrome-manifest",
  "manifest_version": 3,
  "name": "CLearning Enhancer",
  "version": "0.2.0",
  "description": "CLearningを使いやすくする拡張機能",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://udai.c-learning.jp/*"
